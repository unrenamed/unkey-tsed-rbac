{
  "name": "unkey-tsed",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "build": "pnpm run barrels && swc src --out-dir dist -s",
    "barrels": "barrelsby --config .barrelsby.json",
    "start": "pnpm run barrels && node-dev src/index.ts",
    "start:prod": "cross-env NODE_ENV=production node dist/index.js",
    "test": "pnpm run test:lint && pnpm run test:coverage ",
    "test:unit": "cross-env NODE_ENV=test vitest run",
    "test:watch": "cross-env NODE_ENV=test vitest",
    "test:coverage": "cross-env NODE_ENV=test vitest run --coverage",
    "test:lint": "eslint '**/*.{ts,js}'",
    "test:lint:fix": "eslint '**/*.{ts,js}' --fix"
  },
